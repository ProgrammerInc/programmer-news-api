// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Article {
  id            Int    @default(autoincrement()) @id
  title         String
  description   String
  body          String
  url           String
  isActive      Boolean
}

model Feed {
  id            Int    @default(autoincrement()) @id
  title         String
  description   String
  url           String
  isActive      Boolean
}